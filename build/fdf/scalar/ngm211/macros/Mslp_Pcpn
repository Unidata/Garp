line_type		= 1
line_width		= 1
line_label_frequency	= 0
operation		= run
!
! Overlay geopotential height.
fdf			= MSLPress_mb
line_color		= 17
line_type		= 1
line_width		= 2
line_label_frequency	= 1
colorh                  = 17
colorl                  = 17
symbolh                 = H#
symboll                 = L#
operation		= run

! *** End of Parameters ***
